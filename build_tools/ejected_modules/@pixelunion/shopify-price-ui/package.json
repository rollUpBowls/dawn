      },
      {
        "action": "mergeLocale",
        "input": "src/locales/cs.json",
        "output": "locales/cs.json"
      },
      {
        "action": "mergeLocale",
        "input": "src/locales/de.json",
        "output": "locales/de.json"
      },
      {
        "action": "mergeLocale",
        "input": "src/locales/es.json",
        "output": "locales/es.json"
      },
      {
        "action": "mergeLocale",
        "input": "src/locales/fr.json",
        "output": "locales/fr.json"
      },
      {
        "action": "mergeLocale",
        "input": "src/locales/hi.json",
        "output": "locales/hi.json"
      },
      {
        "action": "mergeLocale",
        "input": "src/locales/it.json",
        "output": "locales/it.json"
      },
      {
        "action": "mergeLocale",
        "input": "src/locales/ja.json",
        "output": "locales/ja.json"
      },
      {
        "action": "mergeLocale",
        "input": "src/locales/nl.json",
        "output": "locales/nl.json"
      },
      {
        "action": "mergeLocale",
        "input": "src/locales/pt-BR.json",
        "output": "locales/pt-BR.json"
      },
      {
        "action": "mergeLocale",
        "input": "src/locales/tr.json",
        "output": "locales/tr.json"
      },
      {
        "action": "mergeLocale",
        "input": "src/locales/zh-CN.json",
        "output": "locales/zh-CN.json"
      }
    ]
  }

