{
  "name": "@pixelunion/shopify-price-ui",
  "version": "2.0.4",
  "description": "Pricing display for the product page/card.",
  "main": "dist/index.js",
  "umd": "dist/index.umd.js",
  "umdName": "ShopifyPriceUI",
  "module": "dist/index.es.js",
  "scripts": {
    "build": "node rollup.js",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pixelunion/shopify-price-ui.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "author": "Pixel Union",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "7.13.14",
    "@babel/plugin-external-helpers": "7.12.13",
    "@babel/preset-env": "7.13.12",
    "@rollup/plugin-commonjs": "18.0.0",
    "@rollup/plugin-node-resolve": "11.2.1",
    "babel-eslint": "10.1.0",
    "eslint": "7.23.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-import": "2.22.1",
    "rollup": "2.44.0",
    "rollup-plugin-babel": "4.4.0",
    "stylelint": "13.12.0",
    "stylelint-config-recess-order": "2.3.0",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-config-standard": "21.0.0",
    "stylelint-scss": "3.19.0",
    "uglify-js": "3.13.3"
  },
  "paskitPlugin": {
    "build": [
      {
        "action": "copy",
        "input": "snippets/price-ui-templates.liquid",
        "output": "snippets/price-ui-templates.liquid"
      },
      {
        "action": "mergeLocale",
